<h1>Tracks</h1>
<table>
  <thead>
    <th>Title</th>
    <th>Listen</th>
  </thead>
  <tbody>
    {{#each tracks}}
      <tr>
        <td>{{this.name}}</td>
        <td>
          <audio controls>
            <source src="{{this.preview_url}}" type="audio/ogg">
            <source src="{{this.preview_url}}" type="audio/mpeg">
            Your browser does not support the audio tag.
          </audio>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>